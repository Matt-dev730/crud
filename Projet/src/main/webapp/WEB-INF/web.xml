<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>AddUserServ</servlet-name>
  	<display-name>AddUserServ</display-name>
  	<description></description>
  	<servlet-class>Servlets.AddUserServ</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ControlIdent</servlet-name>
  	<display-name>ControlIdent</display-name>
  	<description></description>
  	<servlet-class>Servlets.ControlIdent</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ListUser</servlet-name>
  	<display-name>ListUser</display-name>
  	<description></description>
  	<servlet-class>Servlets.ListUser</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ContactServlet</servlet-name>
  	<display-name>ContactServlet</display-name>
  	<description></description>
  	<servlet-class>Servlets.ContactServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>AddUserServ</servlet-name>
  	<url-pattern>/addUser</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ControlIdent</servlet-name>
  	<url-pattern>/controlident</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ListUser</servlet-name>
  	<url-pattern>/listUser</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ContactServlet</servlet-name>
  	<url-pattern>/contact</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
  	<welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>

